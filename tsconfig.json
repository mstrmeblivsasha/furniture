{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "target": "es5",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/BurgerBtn/BurgerBtn.tsx",
    "src/utils/connectToDB.ts",
    "src/app/api/catalogue/[category]/route.ts",
    "src/app/api/catalogue/route.ts",
    "src/app/(pages)/dashboard/page.tsx",
    "src/fetch/ClientFetch.ts",
    "src/sections/dashboardCatalogueSection/DashboardCatalogueSection.tsx",
    "src/components/DashboardCatalogueItem/DashboardCatalogueItem.tsx",
    "src/sections/dashboardCatalogueCategorySection/DashboardCatalogueCategorySection.tsx",
    "src/app/(pages)/catalogue/[category]/page.tsx",
    "src/components/DashboardCatalogueCreateForm/DashboardCatalogueCreateForm.jsx",
    "src/app/(pages)/dashboard/register/page.tsx",
    "src/models/catalogueSchema.ts",
    "src/models/userSchema.ts",
    "src/components/DashboardRegisterForm/DashboardRegisterForm.jsx",
    "src/components/DashboardLoginForm/DashboardLoginForm.jsx",
    "src/components/DashboardCatalogueUpdateForm/DashboardCatalogueUpdateForm.jsx",
    "src/context/SiteContext.tsx",
    "src/auth/auth.ts",
    "src/sections/dashboardRegisterSection/DashboardRegisterSection.tsx",
    "src/sections/dashboardLoginSection/DashboardLoginSection.tsx",
    "src/app/(pages)/dashboard/layout.tsx",
    "src/components/LogoutBtn/LogoutBtn.tsx",
  ],
  "exclude": [
    "node_modules"
  ]
}